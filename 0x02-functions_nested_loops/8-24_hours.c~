#include "holberton.h"

/**
 * main - check the code for Holberton School students.
 *
 * Return: Always 0.
 */
int main(void)
{
	jack_bauer(a, b, c, d);
	return (0);
}

void jack_bauer(void)
{
	int a, b, c, d = 0;

	while(a <= 2)
	{
		while (b <= 9)
		{
			while (c <= 5)
			{
				while(d <= 9)
				{
					_putchar(a);
					_putchar(b);
					_putchar(':');
					_putchar(c);
					_putchar(d);
					d++;
				}
				c++;
			}
			b++;
		}
		a++;
	}
}
