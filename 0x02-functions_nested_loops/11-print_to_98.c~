#include "holberton.h"
#include <stdio.h>
/**
 * main - check the code for Holberton School students.
 *
 * Return: Always 0.
 */
int main(void)
{
	print_to_98(0);
	print_to_98(98);
	print_to_98(111);
	print_to_98(81);
	print_to_98(-10);
	return (0);
}

void print_to_98(int n)
{
	int j = n;
	int inic = 98;

	if (j <= inic)
	{
		for (; j < inic; j++)
			printf("%d, ", j);
	}

	if (j >= inic)
	{
		for (; j > inic; j--)
			printf("%d", j);
	}

	if (j == inic)
	{
		printf("%d",j);
	}
	printf("\n");
}
